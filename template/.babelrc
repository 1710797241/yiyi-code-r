{
  "presets": [
    "env",
    "react",
    "stage-0"
  ],

  "plugins": [

    ["transform-decorators-legacy"],
    ["transform-class-properties"],
    [
      "transform-runtime"
    ],
    [
      "syntax-dynamic-import"
    ],
    ["import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true
      }
    ],
    ["import", { "libraryName": "antd-mobile","libraryDirectory": "es", "style": "css" }] // `style: true` 会加载 less 文件

  ]

}